{
  "name": "alecrabbit/php-simple-profiler",
  "type": "library",
  "description": "For functions benchmarking, contain counters and timers classes.",
  "keywords": [
    "counter",
    "timer",
    "simple",
    "profiler",
    "benchmark",
    "microtime"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "alecrabbit/php-accessories": "^0.9",
    "alecrabbit/php-console-colour": "^0.5",
    "alecrabbit/php-console-spinner": "^0.14",
    "alecrabbit/php-helpers": "^0.6",
    "alecrabbit/php-traits": "^0.3",
    "sebastian/exporter": "^3.1"
  },
  "require-dev": {
    "krowinski/bcmath-extended": "^4.2 || ^5.0",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0",
    "symfony/console": "^4.2",
    "symfony/phpunit-bridge": "^4.2",
    "symfony/var-dumper": "^4.2"
  },
  "suggest": {
    "symfony/console": "To use symfony progress bar and colored output."
  },
  "autoload": {
    "psr-4": {
      "AlecRabbit\\": "src"
    },
    "files": [
      "src/Tools/bootstrap.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "AlecRabbit\\Tests\\Tools\\": "tests/"
    }
  },
  "minimum-stability": "alpha"
}
