{
  "name": "alecrabbit/php-simple-profiler",
  "type": "library",
  "description": "For functions benchmarking, contain counters and timers classes.",
  "keywords": [
    "counter",
    "timer",
    "hrtime",
    "simple",
    "profiler",
    "benchmark",
    "microtime"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3",
    "alecrabbit/php-accessories": "^0.12",
    "alecrabbit/php-console-colour": "^0.5",
    "alecrabbit/php-console-spinner": "^0.16",
    "alecrabbit/php-counters": "^0.2",
    "alecrabbit/php-helpers": "^0.6",
    "alecrabbit/php-reports": "^0.2",
    "alecrabbit/php-timers": "^0.1",
    "alecrabbit/php-traits": "^0.3",
    "sebastian/exporter": "^3.1",
    "markrogoyski/math-php": "^0.52.0",
    "webmozart/assert": "^1.4",
    "sehrgut/mb-str-pad": "^1.0"
  },
  "require-dev": {
    "krowinski/bcmath-extended": "^4.2 || ^5.0",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0",
    "symfony/console": "^4.3",
    "symfony/phpunit-bridge": "^4.3",
    "symfony/var-dumper": "^4.3",
    "php-mock/php-mock-phpunit": "^2.4",
    "phpbench/phpbench": "^0.16.9"
  },
  "suggest": {
    "symfony/console": "To use symfony progress bar and colored output."
  },
  "autoload": {
    "psr-4": {
      "AlecRabbit\\Tools\\": "src\\Tools"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlecRabbit\\Tests\\Tools\\": "tests/"
    }
  },
  "minimum-stability": "alpha"
}
