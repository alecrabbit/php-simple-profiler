{
  "name": "alecrabbit/php-simple-profiler",
  "type": "library",
  "description": "PHP simple profiler",
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "alecrabbit/php-helpers": "^0",
    "krowinski/bcmath-extended": "^4.1",
    "nesbot/carbon": "^2.5"
  },
  "require-dev": {
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.4",
    "squizlabs/php_codesniffer": "^3.3",
    "symfony/phpunit-bridge": "^4.1",
    "symfony/var-dumper": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "AlecRabbit\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "prefer-stable": true
}
