{
  "name": "alecrabbit/php-simple-profiler",
  "type": "library",
  "description": "PHP simple profiler",
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "alecrabbit/php-helpers": "^0.1",
    "krowinski/bcmath-extended": "^4.2",
    "nesbot/carbon": "^2.7",
    "lstrojny/functional-php": "^1.8",
    "alecrabbit/php-console-colour": "^0.0.4"
  },
  "require-dev": {
    "alecrabbit/accessories": "^0.1",
    "alecrabbit/php-output-a": "^0.0.1",
    "nunomaduro/collision": "^2.1",
    "phpunit/phpunit": "^7.5",
    "squizlabs/php_codesniffer": "^3.3",
    "symfony/phpunit-bridge": "^4.2",
    "symfony/var-dumper": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "AlecRabbit\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "beta",
  "prefer-stable": true
}
